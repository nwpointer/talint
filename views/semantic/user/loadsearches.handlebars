<div class="block-white">
<h1>select search to load</h1>
<ul>
{{#each savedSearches}}
	<li>
		<a href="#" onclick="loadSearch({{@index}})">
			{{name}}
		</a>
	</li>
{{/each}}
</ul>

<script>
	loadSearch = function(i){
		io.socket.get('/api/user/{{user.id}}',function(res){
			search = res.savedSearches[i];
			console.log(search.data);
			localStorage.setItem('skillTree',search.data);
			location.href ='/user'
		});
		
	}
</script>

</div>